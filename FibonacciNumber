var fib = function(n) {

//Iterative
  /*  var arr = [0,1,1];
    
    for(var i = 3 ; i <= n ; i++) {
      arr[i] = arr[i-1]+arr[i-2];  
    }
    
    return arr[n]; */
  //Recurse  
    if(n === 0) {
        return 0;
    }
    if(n <= 2) {
        return 1;
    }
    
    return fib(n-1)+fib(n-2);
};
